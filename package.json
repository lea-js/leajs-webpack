{
  "name": "leajs-webpack",
  "description": "webpack development and hot reload middleware",
  "version": "0.0.0",
  "homepage": "https://github.com/lea-js/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lea-js/leajs-webpack"
  },
  "engines": {
    "node": "*"
  },
  "main": "lib/plugin.js",
  "files": [
    "lib/*.js"
  ],
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.1",
    "script-runner": "^0.1.7",
    "webpack": "^4.10.0",
    "snapy": "^0.1.3",
    "read-conf": "^0.4.3",
    "leajs-server": "^0.1.1"
  },
  "keywords": [],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "coffee --bare --no-header --compile --output lib/ src/*.coffee",
    "test": "snapy",
    "doc": "toDoc",
    "dev": "leajs --watch",
    "preversion": "npm run test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "eventsource-polyfill": "^0.9.6",
    "memory-fs": "^0.4.1",
    "mime": "^2.3.1",
    "read-conf": "^0.4.3",
    "virtual-module-webpack-plugin": "^0.4.0",
    "webpack-hot-middleware": "^2.22.2"
  },
  "peerDependencies": {
    "webpack": "^4.10.0"
  }
}